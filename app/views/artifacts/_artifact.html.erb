<div id="<%= dom_id artifact %>">
  <div class="card card-header my-1" style="width: 19rem;">
    <div class="card-body">
      <%= link_to image_tag(artifact.artimg.variant(:thumb), width: "500", class: "card-img-top x-auto d-block", alt:"Card image cap"), artifact%>
      <h5 class="card-title mt-2"><%= artifact.name %></h5>
      <p class="card-text"><%= artifact.picture %></p>
    </div>
  </div>
</div>


